{
  "rewrites": [
    {
      "source": "/@vite/(.*)",
      "destination": "/@vite/$1"
    },
    {
      "source": "/@react-refresh",
      "destination": "/@react-refresh"
    },
    {
      "source": "/src/(.*)",
      "destination": "/src/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
